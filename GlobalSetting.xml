<?xml version="1.0" encoding="utf-8"?>
<VitoSDKConfig>
  <!--申请的逻辑服务器端口-->	
  <logicserverport>3001</logicserverport>
  <!--申请的游戏服务器端口-->	  
  <gameserverport>4001</gameserverport>
  <!--服务器IP地址-->	
  <serverip>192.168.1.116</serverip>
  <!--申请的appkey-->
  <appkey>20170307</appkey>
  <!--windows平台文件根目录-->	
  <pcfileroot>D</pcfileroot>
  <!--android平台文件根目录，最终的apk需要有文件读写权限-->
  <androidfileroot>sdcard</androidfileroot>
  <!--视频内容配置列表,只有控制台需要此配置-->	  
  <videoconfigfile>vitoresources/configfile/videoconfig.xml</videoconfigfile>  
  <!--场景内容配置列表,只有控制台需要此配置-->  
  <sceneconfigfile>vitoresources/configfile/sceneconfig_demo.xml</sceneconfigfile>
  <!--视频文件路径-->
  <videopath>vitoresources/vitovideo</videopath>
</VitoSDKConfig>